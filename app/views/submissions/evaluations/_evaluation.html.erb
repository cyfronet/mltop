<% cache_if evaluation.persisted?, evaluation do %>
  <li id="<%= dom_id(evaluation) %>">
    <div class="px-4 py-4 sm:px-6">
      <div class="flex items-center justify-between">
        <div class="truncate text-sm font-medium"><%= evaluation.evaluator.name %></div>
        <div class="ml-2 flex flex-shrink-0">
          <%= mltop_run_status(evaluation) %>
        </div>
      </div>
      <div class="mt-2">
        <%= render "submissions/evaluations/scores", evaluation: , metrics: evaluation.evaluator.metrics %>
      </div>

    </div>
  </li>
<% end %>
