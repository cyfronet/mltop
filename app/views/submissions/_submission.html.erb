<%# locals: (model:) %>

<li class="flex flex-wrap items-center justify-between gap-x-6 gap-y-4 py-5 sm:flex-nowrap">
  <div>
    <p class="text-sm font-semibold leading-6 ">
      <%= link_to model.name, submission_path(model), class: "hover:unerline" %>
    </p>
    <div class="mt-1 flex items-center gap-x-2 text-xs leading-5 text-gray-500">
      <p><%= model.owner.name %> </p>
      <svg viewBox="0 0 2 2" class="h-0.5 w-0.5 fill-current">
        <circle cx="1" cy="1" r="1" />
      </svg>
      <p><time datetime="<%= model.created_at %>"><%= time_ago_in_words model.created_at %> ago</time></p>
    </div>
  </div>
  <dl class="flex w-full flex-none justify-between gap-x-8 sm:w-auto">
    Placeholder for score
  </dl>
</li>
